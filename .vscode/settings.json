{
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "workbench.editor.customLabels.patterns": {
      "**/index.ts": "${dirname}/index",
      "**/index.tsx": "${dirname}/index",
      "**/queries.ts": "${dirname}/queries",
      "**/types.ts": "${dirname}/types"
    },
    "eslint.nodePath": ".pnpm/sdks",
    "eslint.runtime": "node",
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "files.associations": {
      ".env.*.local": "properties",
      ".env.*": "properties"
    },
    "files.exclude": {
      "**/.cache": true,
      "**/.DS_Store": true,
      "**/.editorconfig": true,
      "**/.eslintcache": true,
      "**/.git": true,
      "**/.gitattributes": true,
      "**/.husky": true,
      "**/.pnp.*": true,
      "**/.prettierignore": true,
      "**/node_modules": true,
      "**/pnpm.lock": true
    },
    "search.exclude": {
      "**/dist/": true,
      "**/.pnp.*": true,
      "**/.pnpm": true,
      "**/pnpm-error.log": true,
      "**/pnpm.lock": true,
      "**/node_modules": true
    },
    "terminal.integrated.env.linux": {
      "CACHE_DIR": "${workspaceFolder}/.cache"
    },
    "terminal.integrated.env.osx": {
      "CACHE_DIR": "${workspaceFolder}/.cache"
    },
    "terminal.integrated.env.windows": {
      "CACHE_DIR": "${workspaceFolder}\\.cache"
    },
    "cSpell.ignoreWords": [
      "browserslist",
      "cloudfunctions",
      "corejs",
      "corepack",
      "endregion",
      "entrypoint",
      "envalid",
      "envars",
      "eslintcache",
      "esmodules",
      "esnext",
      "execa",
      "firebaseapp",
      "firestore",
      "globby",
      "hono",
      "identitytoolkit",
      "jamstack",
      "kriasoft",
      "localforage",
      "zustand",
      "tailwindcss",
      "miniflare",
      "nodenext",
      "notistack",
      "oidc",
      "openai",
      "pathinfo",
      "pino",
      "pnpify",
      "reactstarter",
      "refetch",
      "refetchable",
      "relyingparty",
      "sendgrid",
      "signup",
      "sourcemap",
      "spdx",
      "swapi",
      "trpc",
      "tslib",
      "typechecking",
      "vite",
      "vitest",
      "webflow",
      "pnpmpkg",
      "pnpmrc",
      "mapbox",
      "tanstack",
      "youdooh",
      "heroicons"
    ],
    "cSpell.words": [
      "camelcase",
      "composables",
      "filesize",
      "formkit",
      "geohash",
      "hashgroup",
      "headlessui",
      "latlon",
      "luxon",
      "moveend",
      "ngeohash",
      "onboarded",
      "openrtb",
      "pinia",
      "popperjs",
      "rswrp",
      "RUDDERSTACK",
      "signin",
      "svgr",
      "Topup",
      "uidotdev",
      "usehooks",
      "vueuse",
      "zoomend"
    ],
    "[markdown]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "prettier.requireConfig": true
  }